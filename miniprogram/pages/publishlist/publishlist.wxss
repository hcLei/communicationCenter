/* pages/publishlist/publishlist.wxss */
.list-title{
    width: 100%;
    text-align: center;
    color: #333;
    font-size: 32rpx;
    padding: 15rpx 0;
}
.list-content{
    width: 100%;
    height: auto;
}
.list-content .list-item{
    padding: 20rpx;
    background: #fff;
    border-top: 1px solid rgba(0, 0, 0, 0.1);
}
.list-content .list-item:last-child{
    border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}
.container .content-nodata{
  width: 100%;
  text-align: center;
  display: block;
  color: #333;
  margin-top: 300rpx;
}